#include<stdio.h>

int main()
{
    //6. Determine the greatest common divisor of two integers. It can be
   //brute-force, subtraction method, Euclidean-algorithm.


    int a, b;
    printf("Enter number A: ");
    scanf("%d", &a);
    printf("Enter number B: ");
    scanf("%d", &b);

    // 6 12 -- 

    int gcd = 1;

    for( int i = 1; i <= a && i <= b; ++i)
    {
        if(a % i == 0 && b % i == 0)
        gcd = i;
    }

    printf("GCD is %d", gcd);


    


}
